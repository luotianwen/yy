  <script type="text/javascript" src="/static/js/settled/settled.js"></script>
<script>
	$("#check").click(function() {
		var f=$("input[type='checkbox']").is(':checked');
		if(f){
			$("#nextbtn").removeClass("btn_c8c8c8").addClass("btn_038a2a");
			$("#nextbtn").attr("disabled",false);
		}
		else{
			$("#nextbtn").removeClass("btn_038a2a").addClass("btn_c8c8c8");
			$("#nextbtn").attr("disabled",true);
		}
	});
	$("#nextbtn").click(function() {
		$.post("/settled/check_in_save",function(data){
			if(data.RESPONSE_STATE=='200'){
				window.location.href="/settled/check_in_1";
			}else{
				layer.alert("系统错误，请刷新后重试", {
					icon : 0
				});
			}
		})
	});
</script>
